EATA;AAUA,mBAAA;AAVA,OAAA;AAYA;AAxCA,GAXA,CAsDA;AAEA;AACA;AACA;AACA;;AA3DA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <h1>Create an Event</h1>\r\n\r\n    <form @submit.prevent=\"createEvent\">\r\n      <label>Select a category</label>\r\n      <select v-model=\"event.category\">\r\n        <option v-for=\"cat in categories\" :key=\"cat\">{{ cat }}</option>\r\n      </select>\r\n\r\n      <h3>Name & describe your event</h3>\r\n      <div class=\"field\">\r\n        <label>Title</label>\r\n        <input\r\n          v-model=\"event.title\"\r\n          type=\"text\"\r\n          placeholder=\"Add an event title\"\r\n        />\r\n      </div>\r\n\r\n      <div class=\"field\">\r\n        <label>Description</label>\r\n        <input\r\n          v-model=\"event.description\"\r\n          type=\"text\"\r\n          placeholder=\"Add a description\"\r\n        />\r\n      </div>\r\n\r\n      <h3>Where is your event?</h3>\r\n      <div class=\"field\">\r\n        <label>Location</label>\r\n        <input\r\n          v-model=\"event.location\"\r\n          type=\"text\"\r\n          placeholder=\"Add a location\"\r\n        />\r\n      </div>\r\n\r\n      <h3>When is your event?</h3>\r\n\r\n      <div class=\"field\">\r\n        <label>Date</label>\r\n        <datepicker v-model=\"event.date\" placeholder=\"Select a date\" />\r\n      </div>\r\n\r\n      <div class=\"field\">\r\n        <label>Select a time</label>\r\n        <select v-model=\"event.time\">\r\n          <option v-for=\"time in times\" :key=\"time\">{{ time }}</option>\r\n        </select>\r\n      </div>\r\n\r\n      <input type=\"submit\" class=\"button -fill-gradient\" value=\"Submit\" />\r\n    </form>\r\n  </div>\r\n</template>\r\n<script>\r\nimport datepicker from 'vuejs-datepicker'\r\nexport default {\r\n  components: {\r\n    datepicker\r\n  },\r\n  data() {\r\n    return {\r\n      event: this.createFreshEventObject(),\r\n      categories: this.$store.state.categories,\r\n      times: this.createTimes()\r\n    }\r\n  },\r\n  methods: {\r\n    createEvent() {\r\n      this.$store\r\n        .dispatch('createEvent', this.event)\r\n        .then(() => {\r\n          //\r\n          this.$router.push({\r\n            name: 'event-show',\r\n            params: { id: this.event.id }\r\n          })\r\n          this.event = this.createFreshEventObject()\r\n        })\r\n        .catch(() => {\r\n          console.log('There is a problem with API, cant connect server')\r\n        })\r\n\r\n      console.log(this.event)\r\n    },\r\n    createTimes() {\r\n      const times = []\r\n      for (let i = 1; i <= 24; i++) {\r\n        times.push(i + ':00') // 1:00, 2:00\r\n      }\r\n      return times\r\n    },\r\n    createFreshEventObject() {\r\n      const user = this.$store.state.user\r\n      const id = Math.floor(Math.random() * 100000000)\r\n      return {\r\n        id: id,\r\n        user: user,\r\n        category: '',\r\n        organizer: user,\r\n        title: '',\r\n        description: '',\r\n        location: '',\r\n        date: '',\r\n        time: '',\r\n        attendees: []\r\n      }\r\n    }\r\n  }\r\n}\r\n// POST /events\r\n\r\n// 1) add mutations - x\r\n// 2) add Action\r\n// 3) Post request with axios in our AppApi\r\n// 4) Call or action from our component (dispatch)\r\n</script>\r\n"],"sourceRoot":"src/views"}]}